    "antd": "^4.20.7",
    "apexcharts": "^3.35.0",
    "aws-amplify": "^4.3.24",
    "axios": "^0.27.2",
    "chart.js": "^3.8.0",
    "chat_application": "^1.0.0",
    "eslint": "^8.17.0",
    "framer-motion": "^6.2.9",
    "history": "^4.10.1",
    "html2canvas": "^1.4.1",
